#include <bits/stdc++.h>
using namespace std;
int main()
{
    set <int> S1 = {1,2,3,4,5,2,8};
    bool flag = false;
    int n;
    cout << "Write the number to check for: ";
    cin>>n;
    set<int>::iterator it;
    
    for (it = S1.begin(); it != S1.end(); it++){
        if (n == (*it)){
            flag = true;
            
        }
    }
    if (flag == true){
        cout<<"TRUE";
    }
    else{
        cout << "FALSE";
    }
    
}
